install: ; chmod 755 bin/install.sh && sh bin/install.sh

configure: ; chmod 755 bin/config.sh && sh bin/config.sh

startServer: ; chmod 755 bin/startServer.sh && sh bin/startServer.sh

stopServer: ; chmod 755 bin/stopServer.sh && sh bin/stopServer.sh

reset: ; chmod 755 bin/reset.sh && sh bin/reset.sh